package vos
{
	public class ElementVO
	{
		//----------------基础属性
		public var type:String;
		public var x:int;
		public var y:int;
		public var width:int;
		public var height:int;
		public var actions:Vector.<ActionVO>;
		
		
		//----------------文本用资源
		[Bindable]
		public var label:String = "label";
		
		//-----------------图片资源
		[Bindable]
		public var source:Object = "assets/image.png";
	}
}